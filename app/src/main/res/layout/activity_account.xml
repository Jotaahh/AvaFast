<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="accountVM" type="com.gustavosapplication.app.modules.account.data.viewmodel.AccountVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearColumnrectanglefourteen"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyleblack_900">
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearAccount"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStyleblack_900">
            <FrameLayout android:gravity="start"
                android:id="@+id/frameStackrectanglefourteen"
                android:layout_height="@dimen/_150pxv" android:layout_width="match_parent">
                <View android:background="@color/gray_900"
                    android:id="@+id/viewRectangleFourteen"
                    android:layout_gravity="top|start"
                    android:layout_height="@dimen/_108pxv"
                    android:layout_marginBottom="@dimen/_10pxv" android:layout_width="@dimen/_414pxh"
                    tools:ignore="VisualLintBounds" />
                <FrameLayout android:id="@+id/frameAvatar"
                    android:layout_gravity="bottom|center"
                    android:layout_height="@dimen/_117pxv"
                    android:layout_marginEnd="@dimen/_150pxh"
                    android:layout_marginStart="@dimen/_150pxh"
                    android:layout_marginTop="@dimen/_10pxv" android:layout_width="@dimen/_114pxh">
                    <ImageView android:id="@+id/imageAndriykopodiln"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_114pxv"
                        android:layout_marginBottom="@dimen/_3pxv"
                        android:layout_width="match_parent"
                        android:scaleType="centerCrop"
                        circular="@{true}"
                        imageRes="@{@drawable/img_andriykopodiln}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_andriykopodiln"/>

                    <ImageButton
                        android:id="@+id/btnGroup191"
                        android:layout_width="@dimen/_24pxh"
                        android:layout_height="@dimen/_24pxh"
                        android:layout_gravity="bottom|end"
                        android:layout_marginStart="@dimen/_12pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_marginEnd="@dimen/_12pxh"
                        android:background="@drawable/rectangle_bg_white_a700_radius_12"
                        android:contentDescription="@string/lbl_16"
                        android:paddingHorizontal="@dimen/_5pxh"
                        android:paddingVertical="@dimen/_5pxv"
                        android:scaleType="fitCenter"
                        android:src="@drawable/img_group191"
                        tools:ignore="ContentDescription,TouchTargetSizeCheck" />
                </FrameLayout>
            </FrameLayout>

            <TextView
                android:id="@+id/txtNeymarJr"
                style="@style/txtPoppinssemibold15"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_marginStart="@dimen/_37pxh"
                android:layout_marginTop="@dimen/_4pxv"
                android:layout_marginEnd="@dimen/_37pxh"
                android:ellipsize="end"
                android:lineSpacingExtra="@dimen/_8pxh"
                android:singleLine="true"
                android:text="@{accountVM.accountModel.txtNeymarJr}"
                tools:ignore="TextSizeCheck"
                tools:text="@string/lbl_neymar_junior" />
            <LinearLayout android:id="@+id/linearAboutMe"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_36pxh"
                android:layout_marginStart="@dimen/_37pxh"
                android:layout_marginTop="@dimen/_47pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:id="@+id/linearRowvector"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.91"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageVector"
                        android:layout_height="@dimen/_20pxh"
                        android:layout_width="@dimen/_20pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_vector_white_a700"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_vector_white_a700"/>

                    <TextView
                        android:id="@+id/txtSobremim"
                        style="@style/txtPoppinssemibold12"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_15pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_weight="0.78"
                        android:ellipsize="end"
                        android:letterSpacing="0.02"
                        android:singleLine="true"
                        android:text="@{accountVM.accountModel.txtSobremim}"
                        tools:ignore="NestedWeights,TextSizeCheck"
                        tools:text="@string/lbl_sobre_mim" />
                </LinearLayout>
                <ImageView android:id="@+id/imageArrowright"
                    android:layout_height="@dimen/_18pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_225pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_10pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowright_white_a700_18x10"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowright_white_a700_18x10"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearMyFav"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_37pxh"
                android:layout_marginStart="@dimen/_37pxh"
                android:layout_marginTop="@dimen/_18pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:id="@+id/linearRowvectorOne"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.92"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageVectorOne"
                        android:layout_height="@dimen/_16pxh"
                        android:layout_marginTop="@dimen/_2pxv"
                        android:layout_width="@dimen/_18pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_vector_white_a700_16x18"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_vector_white_a700_16x18"/>

                    <TextView
                        android:id="@+id/txtMeusfavoritos"
                        style="@style/txtPoppinssemibold12"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_16pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_weight="0.84"
                        android:ellipsize="end"
                        android:letterSpacing="0.02"
                        android:singleLine="true"
                        android:text="@{accountVM.accountModel.txtMeusfavoritos}"
                        tools:ignore="NestedWeights,TextSizeCheck"
                        tools:text="@string/lbl_meus_favoritos" />
                </LinearLayout>
                <ImageView android:id="@+id/imageArrowrightOne"
                    android:layout_height="@dimen/_18pxh"
                    android:layout_marginStart="@dimen/_200pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_10pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowright_white_a700_18x10"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowright_white_a700_18x10"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearMyAddress"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_37pxh"
                android:layout_marginStart="@dimen/_37pxh"
                android:layout_marginTop="@dimen/_19pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:id="@+id/linearRowgroup203"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.9"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageGroup203"
                        android:layout_height="@dimen/_20pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_width="@dimen/_14pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_group203"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_group203"/>

                    <TextView
                        android:id="@+id/txtEndereo"
                        style="@style/txtPoppinssemibold12"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_17pxh"
                        android:layout_marginTop="@dimen/_2pxv"
                        android:layout_weight="0.81"
                        android:ellipsize="end"
                        android:letterSpacing="0.02"
                        android:singleLine="true"
                        android:text="@{accountVM.accountModel.txtEndereo}"
                        tools:ignore="NestedWeights,TextSizeCheck"
                        tools:text="@string/lbl_endere_o" />
                </LinearLayout>
                <ImageView android:id="@+id/imageArrowrightTwo"
                    android:layout_height="@dimen/_18pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_235pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_10pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowright_white_a700_18x10"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowright_white_a700_18x10"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearSignout"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_39pxh"
                android:layout_marginStart="@dimen/_39pxh"
                android:layout_marginTop="@dimen/_55pxv"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <ImageView android:id="@+id/imageArrowleft"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_width="@dimen/_21pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowleft"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>

                <TextView
                    android:id="@+id/txtSairdaconta"
                    style="@style/txtPoppinssemibold12"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_14pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_weight="0.8"
                    android:ellipsize="end"
                    android:letterSpacing="0.02"
                    android:singleLine="true"
                    android:text="@{accountVM.accountModel.txtSairdaconta}"
                    tools:ignore="TextSizeCheck"
                    tools:text="@string/lbl_sair_da_conta" />
            </LinearLayout>
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearNavigation"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_381pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStyle_1"
                tools:ignore="VisualLintBounds">

                <TextView
                    android:id="@+id/txtAvaFast"
                    style="@style/txtPoppinsmedium20"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginStart="@dimen/_34pxh"
                    android:layout_marginTop="@dimen/_32pxv"
                    android:layout_marginEnd="@dimen/_34pxh"
                    android:layout_marginBottom="@dimen/_17pxv"
                    android:ellipsize="end"
                    android:gravity="end"
                    android:singleLine="true"
                    android:text="@{accountVM.accountModel.txtAvaFast}"
                    tools:ignore="TextSizeCheck,VisualLintBounds"
                    tools:text="@string/lbl_avafast" />
            </LinearLayout>
        </LinearLayout>
        <FrameLayout android:id="@+id/frameBottombar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" style="@style/groupStylegray_900">
            <LinearLayout android:id="@+id/linearRowgroup"
                android:layout_height="match_parent"
                android:layout_marginBottom="@dimen/_22pxv"
                android:layout_marginTop="@dimen/_22pxv"
                android:layout_width="match_parent" android:orientation="horizontal"
                tools:ignore="VisualLintBounds">
                <ImageView android:id="@+id/imageGroup"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_21pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_37pxh"
                    android:layout_width="@dimen/_22pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_group_white_a700"
                    tools:ignore="ContentDescription,VisualLintBounds" tools:src="@drawable/img_group_white_a700"/>
                <ImageView android:id="@+id/imageVectorTwo"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_22pxh"
                    android:layout_marginStart="@dimen/_73pxh"
                    android:layout_width="@dimen/_22pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_vector_white_a700_22x22"
                    tools:ignore="ContentDescription,VisualLintBounds" tools:src="@drawable/img_vector_white_a700_22x22"/>
                <ImageView android:id="@+id/imageVectorThree"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_18pxh"
                    android:layout_marginBottom="@dimen/_2pxv"
                    android:layout_marginEnd="@dimen/_164pxh"
                    android:layout_marginStart="@dimen/_73pxh"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:layout_width="@dimen/_22pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_vector_white_a700_18x22"
                    tools:ignore="ContentDescription,VisualLintBounds" tools:src="@drawable/img_vector_white_a700_18x22"/>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
