<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="productDetailVM" type="com.gustavosapplication.app.modules.productdetail.data.viewmodel.ProductDetailVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearProductdetail"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyleblack_900">
        <FrameLayout android:id="@+id/frameProductImage"
            android:layout_height="@dimen/_388pxv" android:layout_width="match_parent">
            <FrameLayout android:id="@+id/frameStackellipsesixteen"
                android:layout_gravity="top|start"
                android:layout_height="@dimen/_333pxv"
                android:layout_marginBottom="@dimen/_10pxv" android:layout_width="match_parent">
                <ImageView android:id="@+id/imageEllipseSixteen"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_333pxh"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_ellipse16}"
                    tools:ignore="ContentDescription,VisualLintBounds" tools:src="@drawable/img_ellipse16"/>
                <ImageView android:id="@+id/imageArrowright"
                    android:layout_gravity="top|start"
                    android:layout_height="@dimen/_16pxh"
                    android:layout_marginBottom="@dimen/_31pxv"
                    android:layout_marginEnd="@dimen/_17pxh"
                    android:layout_marginStart="@dimen/_17pxh"
                    android:layout_marginTop="@dimen/_31pxv"
                    android:layout_width="@dimen/_23pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowright"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowright"/>
            </FrameLayout>
            <ImageView android:id="@+id/imageLime2311"
                android:layout_gravity="bottom|center"
                android:layout_height="@dimen/_324pxh"
                android:layout_marginEnd="@dimen/_44pxh"
                android:layout_marginStart="@dimen/_44pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="@dimen/_324pxh"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_lime2311}"
                tools:ignore="ContentDescription,VisualLintBounds" tools:src="@drawable/img_lime2311"/>
            <ImageView android:id="@+id/imageImageFive"
                android:layout_gravity="bottom|center"
                android:layout_height="@dimen/_300pxh"
                android:layout_marginEnd="@dimen/_32pxh"
                android:layout_marginStart="@dimen/_32pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="@dimen/_350pxh"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_image5}"
                tools:ignore="ContentDescription,VisualLintBounds" tools:src="@drawable/img_image5"/>
            <FrameLayout android:id="@+id/frameStackellipsesixteenOne"
                android:layout_gravity="top|center"
                android:layout_height="@dimen/_78pxv"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_marginEnd="@dimen/_153pxh"
                android:layout_marginStart="@dimen/_153pxh" android:layout_width="@dimen/_103pxh">
                <View
                    android:background="@drawable/rectangle_bg_white_a700_radius_51_5"
                    android:id="@+id/viewEllipseSixteenOne"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_78pxv" android:layout_width="@dimen/_103pxh"/>
                <ImageView android:id="@+id/imageLogococobambu"
                    android:layout_gravity="top|center"
                    android:layout_height="@dimen/_75pxh"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginEnd="@dimen/_7pxh"
                    android:layout_marginStart="@dimen/_7pxh"
                    android:layout_width="@dimen/_88pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_logococobambu}"
                    tools:ignore="ContentDescription,VisualLintBounds" tools:src="@drawable/img_logococobambu"/>
            </FrameLayout>
        </FrameLayout>
        <LinearLayout android:gravity="end"
            android:id="@+id/linearColumnvector"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_33pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylegray_1">
            <ImageView android:id="@+id/imageVector"
                android:layout_height="@dimen/_18pxh"
                android:layout_marginEnd="@dimen/_21pxh"
                android:layout_marginStart="@dimen/_21pxh"
                android:layout_marginTop="@dimen/_33pxv"
                android:layout_width="@dimen/_20pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_vector_white_a700_18x20"
                tools:ignore="ContentDescription" tools:src="@drawable/img_vector_white_a700_18x20"/>

            <TextView
                android:id="@+id/txtCocoBambu"
                style="@style/txtPoppinssemibold20"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="start"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_marginEnd="@dimen/_266pxh"
                android:ellipsize="end"
                android:singleLine="true"
                android:text="@{productDetailVM.productDetailModel.txtCocoBambu}"
                tools:ignore="TextSizeCheck"
                tools:text="@string/lbl_coco_bambu" />

            <TextView
                android:id="@+id/txtFrutosdoMar"
                style="@style/txtPoppinsmedium12"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="start"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_2pxv"
                android:layout_marginEnd="@dimen/_315pxh"
                android:ellipsize="end"
                android:singleLine="true"
                android:text="@{productDetailVM.productDetailModel.txtFrutosdoMar}"
                tools:ignore="TextSizeCheck"
                tools:text="@string/lbl_frutos_do_mar" />
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRatingsReviews"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_212pxh"
                android:layout_marginStart="@dimen/_17pxh"
                android:layout_marginTop="@dimen/_8pxv"
                android:layout_width="wrap_content" android:orientation="horizontal">

                <TextView
                    android:id="@+id/txtFortyFive"
                    style="@style/txtPoppinsmedium12"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.11"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@{productDetailVM.productDetailModel.txtFortyFive}"
                    tools:ignore="TextSizeCheck"
                    tools:text="@string/lbl_4_5" />
                <ImageView android:id="@+id/imageGroup101"
                    android:layout_height="@dimen/_14pxh"
                    android:layout_marginBottom="@dimen/_3pxv"
                    android:layout_marginStart="@dimen/_2pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_87pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_group101"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_group101"/>

                <TextView
                    android:id="@+id/txt89reviews"
                    style="@style/txtPoppinsmedium12"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_3pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_weight="0.41"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@{productDetailVM.productDetailModel.txt89reviews}"
                    tools:ignore="TextSizeCheck"
                    tools:text="@string/lbl_89_reviews" />
            </LinearLayout>

            <TextView
                android:id="@+id/txtDescription"
                style="@style/txtPoppinsregular12"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_17pxv"
                android:layout_marginEnd="@dimen/_16pxh"
                android:gravity="center_horizontal"
                android:text="@{productDetailVM.productDetailModel.txtDescription}"
                tools:ignore="TextSizeCheck,VisualLintLongText"
                tools:text="@string/msg_considerado_o_m" />

            <LinearLayout
                android:id="@+id/linearPrimaryButton"
                style="@style/groupStylecornerRadius_1"
                android:layout_width="match_parent"
                android:layout_height="49dp"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_74pxv"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginBottom="@dimen/_36pxv"
                android:gravity="end"
                android:orientation="horizontal"
                tools:ignore="VisualLintBounds">

                <TextView
                    android:id="@+id/txtAdicionarAvali"
                    style="@style/txtPoppinsmedium15_1"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_113pxh"
                    android:layout_marginTop="@dimen/_19pxv"
                    android:layout_marginBottom="@dimen/_17pxv"
                    android:layout_weight="0.86"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@{productDetailVM.productDetailModel.txtAdicionarAvali}"
                    android:textColor="#FFFDE7"
                    tools:ignore="TextSizeCheck,TextContrastCheck"
                    tools:text="@string/msg_adicionar_avali" />

                <ImageView
                    android:id="@+id/imageImageEleven"
                    android:layout_width="@dimen/_25pxh"
                    android:layout_height="@dimen/_28pxh"
                    android:layout_marginStart="@dimen/_73pxh"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_marginEnd="@dimen/_15pxh"
                    android:layout_marginBottom="@dimen/_16pxv"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_image11"
                    tools:ignore="ContentDescription,ImageContrastCheck,VisualLintBounds"
                    tools:src="@drawable/img_image11" />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
