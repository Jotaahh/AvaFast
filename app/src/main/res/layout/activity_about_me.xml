<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="aboutMeVM" type="com.gustavosapplication.app.modules.aboutme.data.viewmodel.AboutMeVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearAboutme"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyleblack_900">
        <LinearLayout android:gravity="start"
            android:id="@+id/linearTitleBar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="horizontal" style="@style/groupStyleblack_900">
            <ImageView android:id="@+id/imageArrowright"
                android:layout_height="@dimen/_20pxh"
                android:layout_marginBottom="@dimen/_43pxv"
                android:layout_marginStart="@dimen/_17pxh"
                android:layout_marginTop="@dimen/_50pxv"
                android:layout_width="@dimen/_23pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_arrowright"
                tools:ignore="ContentDescription" tools:src="@drawable/img_arrowright"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtSobremim"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_42pxv"
                android:layout_marginEnd="@dimen/_170pxh"
                android:layout_marginStart="@dimen/_100pxh"
                android:layout_marginTop="@dimen/_45pxv"
                android:layout_weight="0.82" android:layout_width="0dp"
                android:letterSpacing="0.03" android:singleLine="true"
                android:text="@{aboutMeVM.aboutMeModel.txtSobremim}"
                style="@style/txtPoppinsmedium18" tools:text="@string/lbl_sobre_mim"/>
        </LinearLayout>
        <TextView android:ellipsize="end"
            android:id="@+id/txtDetalhesPessoa"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{aboutMeVM.aboutMeModel.txtDetalhesPessoa}"
            style="@style/txtPoppinssemibold18" tools:text="@string/msg_detalhes_pessoa"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearRowvector"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_15pxh"
            android:layout_marginStart="@dimen/_15pxh"
            android:layout_marginTop="@dimen/_13pxv"
            android:layout_width="match_parent"
            android:orientation="horizontal" style="@style/groupStylegray_900cornerRadius">
            <ImageView android:id="@+id/imageVector"
                android:layout_height="@dimen/_24pxh"
                android:layout_marginBottom="@dimen/_18pxv"
                android:layout_marginStart="@dimen/_17pxh"
                android:layout_marginTop="@dimen/_18pxv"
                android:layout_width="@dimen/_24pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_vector_white_a700"
                tools:ignore="ContentDescription" tools:src="@drawable/img_vector_white_a700"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtNeymarDaSilva"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_16pxv"
                android:layout_marginEnd="@dimen/_132pxh"
                android:layout_marginStart="@dimen/_21pxh"
                android:layout_marginTop="@dimen/_20pxv"
                android:layout_weight="0.89" android:layout_width="0dp"
                android:letterSpacing="0.02" android:singleLine="true"
                android:text="@{aboutMeVM.aboutMeModel.txtNeymarDaSilva}"
                style="@style/txtPoppinsmedium15_1" tools:text="@string/msg_neymar_da_silva"/>
        </LinearLayout>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearRowvectorOne"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_15pxh"
            android:layout_marginStart="@dimen/_15pxh"
            android:layout_marginTop="@dimen/_5pxv"
            android:layout_width="match_parent"
            android:orientation="horizontal" style="@style/groupStylegray_900cornerRadius">
            <ImageView android:id="@+id/imageVectorOne"
                android:layout_height="@dimen/_17pxh"
                android:layout_marginBottom="@dimen/_20pxv"
                android:layout_marginStart="@dimen/_18pxh"
                android:layout_marginTop="@dimen/_21pxv"
                android:layout_width="@dimen/_23pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_vector_white_a700_17x23"
                tools:ignore="ContentDescription" tools:src="@drawable/img_vector_white_a700_17x23"/>
            <TextView android:ellipsize="end" android:id="@+id/txtEmail"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_16pxv"
                android:layout_marginEnd="@dimen/_87pxh"
                android:layout_marginStart="@dimen/_21pxh"
                android:layout_marginTop="@dimen/_20pxv"
                android:layout_weight="0.91" android:layout_width="0dp"
                android:letterSpacing="0.02" android:singleLine="true"
                android:text="@{aboutMeVM.aboutMeModel.txtEmail}"
                style="@style/txtPoppinsmedium15_1" tools:text="@string/msg_neymar_jogador"/>
        </LinearLayout>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearRowvectorTwo"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_15pxh"
            android:layout_marginStart="@dimen/_15pxh"
            android:layout_marginTop="@dimen/_5pxv"
            android:layout_width="match_parent"
            android:orientation="horizontal" style="@style/groupStylegray_900cornerRadius">
            <ImageView android:id="@+id/imageVectorTwo"
                android:layout_height="@dimen/_23pxh"
                android:layout_marginBottom="@dimen/_19pxv"
                android:layout_marginStart="@dimen/_18pxh"
                android:layout_marginTop="@dimen/_18pxv"
                android:layout_width="@dimen/_22pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_vector"
                tools:ignore="ContentDescription" tools:src="@drawable/img_vector"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtLanguage"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_18pxv"
                android:layout_marginEnd="@dimen/_181pxh"
                android:layout_marginStart="@dimen/_21pxh"
                android:layout_marginTop="@dimen/_18pxv"
                android:layout_weight="0.86" android:layout_width="0dp"
                android:letterSpacing="0.02" android:singleLine="true"
                android:text="@{aboutMeVM.aboutMeModel.txtLanguage}"
                style="@style/txtPoppinsmedium15_1" tools:text="@string/msg_1_202_555_0"/>
        </LinearLayout>
        <TextView android:ellipsize="end"
            android:id="@+id/txtTrocaraSenha"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_33pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{aboutMeVM.aboutMeModel.txtTrocaraSenha}"
            style="@style/txtPoppinssemibold18_1" tools:text="@string/lbl_trocar_a_senha"/>
        <EditText
            android:background="@drawable/rectangle_bg_gray_900_radius_5"
            android:drawablePadding="@dimen/_27pxh"
            android:drawableStart="@drawable/img_group_5"
            android:ellipsize="end"
            android:hint="@string/lbl_senha_atual"
            android:id="@+id/etGroupNine" android:inputType="text"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_15pxh"
            android:layout_marginStart="@dimen/_15pxh"
            android:layout_marginTop="@dimen/_16pxv"
            android:layout_width="match_parent"
            android:letterSpacing="0.02"
            android:paddingBottom="@dimen/_17pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_17pxh"
            android:paddingTop="@dimen/_19pxv" android:singleLine="true"
            android:text="@={aboutMeVM.aboutMeModel.etGroupNineValue}"
            android:textColorHint="@color/white_A700"
            style="@style/etSolidRounded" tools:ignore="Autofill"/>
        <ImageView android:id="@+id/imageGroupEight"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_60pxh"
            android:layout_marginEnd="@dimen/_15pxh"
            android:layout_marginStart="@dimen/_15pxh"
            android:layout_marginTop="@dimen/_5pxv"
            android:layout_width="@dimen/_380pxh"
            android:scaleType="fitXY" android:src="@drawable/img_group8"
            tools:ignore="ContentDescription" tools:src="@drawable/img_group8"/>
        <EditText
            android:background="@drawable/rectangle_bg_gray_900_radius_5"
            android:drawablePadding="@dimen/_27pxh"
            android:drawableStart="@drawable/img_group_5"
            android:ellipsize="end"
            android:hint="@string/lbl_confirmar_senha"
            android:id="@+id/etGroupTen" android:inputType="text"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_15pxh"
            android:layout_marginStart="@dimen/_15pxh"
            android:layout_marginTop="@dimen/_5pxv"
            android:layout_width="match_parent"
            android:letterSpacing="0.02"
            android:paddingBottom="@dimen/_18pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_17pxh"
            android:paddingTop="@dimen/_18pxv" android:singleLine="true"
            android:text="@={aboutMeVM.aboutMeModel.etGroupTenValue}"
            android:textColorHint="@color/white_A700"
            style="@style/etSolidRounded" tools:ignore="Autofill"/>
        <androidx.appcompat.widget.AppCompatButton
            android:ellipsize="end" android:gravity="center"
            android:id="@+id/btnProximo"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_14pxv"
            android:layout_marginEnd="@dimen/_15pxh"
            android:layout_marginStart="@dimen/_15pxh"
            android:layout_marginTop="@dimen/_173pxv"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_18pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_30pxh"
            android:paddingTop="@dimen/_18pxv" android:singleLine="true"
            android:text="@string/lbl_proximo"
            android:textAllCaps="false" style="@style/btnSolidRounded" tools:text="@string/lbl_proximo"/>
    </LinearLayout>
</layout>
