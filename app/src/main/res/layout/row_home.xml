                <FrameLayout android:id="@+id/frameStackvector"
                    android:layout_height="@dimen/_16pxv"
                    android:layout_marginBottom="@dimen/_81pxv"
                    android:layout_marginStart="@dimen/_23pxh" android:layout_width="@dimen/_18pxh">
                    <ImageView android:id="@+id/imageVectorThree"
