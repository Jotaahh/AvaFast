            </LinearLayout>
            <ImageView android:id="@+id/imageStarReviewTwo"
                android:layout_height="@dimen/_16pxh"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_width="@dimen/_105pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_starreview"
                tools:ignore="ContentDescription" tools:src="@drawable/img_starreview"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtCocoBambuOne"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_38pxh"
                android:layout_marginStart="@dimen/_38pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{homeRowModel.txtCocoBambuOne}"
                style="@style/txtPoppinssemibold15_1" tools:text="@string/lbl_nau"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtFrutosdoMarOne"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_11pxh"
                android:layout_marginStart="@dimen/_11pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{homeRowModel.txtFrutosdoMarOne}"
